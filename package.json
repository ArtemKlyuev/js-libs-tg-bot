{
  "name": "js-libs-tg-bot",
  "version": "1.0.0",
  "license": "MIT",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "proxy": "docker-compose up",
    "add:root": "yarn add -W -D -E",
    "dev:server:client": "yarn lerna run dev --scope='{server,client}' --parallel",
    "package:bot": "yarn workspace bot",
    "package:client": "yarn workspace client",
    "package:common": "yarn workspace common",
    "package:server": "yarn workspace server"
  },
  "devDependencies": {
    "eslint": "8.27.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "lerna": "6.0.3",
    "lint-staged": "13.0.3",
    "nodemon": "2.0.20",
    "patch-package": "6.5.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "type-fest": "3.2.0",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "fuse.js": "6.6.2"
  }
}
