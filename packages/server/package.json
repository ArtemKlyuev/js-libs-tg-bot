{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "private": "true",
  "type": "module",
  "typesVersions": {
    "*": {
      "types": [
        "src/routes/types.ts"
      ]
    }
  },
  "scripts": {
    "postinstall": "patch-package",
    "typecheck": "tsc --noEmit --pretty",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "build": "rm -rf build && ts-node --esm scripts/build.ts"
  },
  "dependencies": {
    "@fastify/auth": "4.2.0",
    "@fastify/awilix": "3.0.2",
    "@fastify/basic-auth": "5.0.0",
    "@fastify/cors": "8.2.1",
    "@sweet-monads/either": "3.2.0",
    "awilix": "8.0.1",
    "common": "1.0.0",
    "fastify": "4.17.0",
    "zod": "3.21.4",
    "zod-to-json-schema": "3.21.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.16",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "pino-pretty": "10.0.0",
    "tsconfig-paths": "4.1.0"
  }
}
